  "private": true,
  "scripts": {
    "test": "nodemon --ext js --exec 'clear; tape tests/**.*.js | ./node_modules/.bin/tap-spec'",
    "testmin": "nodemon --ext js --exec 'clear; tape tests/**.*.js |./node_modules/.bin/tap-min'",
